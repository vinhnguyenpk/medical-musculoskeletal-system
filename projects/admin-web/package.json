{
  "name": "admin-web",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "node --openssl-legacy-provider --max-old-space-size=8192 ./node_modules/.bin/umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .tslll,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "start:dev": "NODE_OPTIONS=--openssl-legacy-provider cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "src/**/*.less": "stylelint --syntax less",
    "src/**/*.{tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-card": "^1.7.0",
    "@ant-design/pro-descriptions": "^1.9.30",
    "@ant-design/pro-layout": "^6.4.19",
    "@ant-design/pro-table": "^2.56.9",
    "@ckeditor/ckeditor5-react": "^5.0.2",
    "@types/html2canvas": "^1.0.0",
    "@umijs/hooks": "^1.9.3",
    "@umijs/plugin-esbuild": "^1.4.1",
    "antd": "^4.9.1",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "chart.js": "^3.5.1",
    "classnames": "^2.2.6",
    "html2canvas": "^1.4.1",
    "lodash": "^4.17.11",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "omit.js": "^2.0.2",
    "p-limit": "^3.0.2",
    "path-to-regexp": "2.4.0",
    "puppeteer": "^17.0.0",
    "qs": "^6.9.0",
    "react": "^16.14.0",
    "react-color": "^2.19.3",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.4",
    "react-image-lightbox": "^5.1.1",
    "react-modal": "^3.11.1",
    "react-papaparse": "^3.11.0",
    "react-qr-code": "^2.0.7",
    "react-to-print": "^2.14.4",
    "react-use-websocket": "3.0.0",
    "reactcss": "^1.2.3",
    "true": "^0.0.4",
    "umi": "^3.5.20",
    "umi-request": "^1.0.8",
    "use-merge-value": "^1.0.1",
    "web3": "^1.3.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^6.1.0",
    "@types/uuid": "^8.3.3",
    "@umijs/fabric": "^2.2.0",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^1.8.27",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "postcss": "^8.4.14",
    "prettier": "^2.0.1",
    "pro-download": "1.0.1"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
