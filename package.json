{
  "name": "medical-musculoskeletal-system",
  "version": "0.0.0",
  "license": "MIT",
  "description": "",
  "workspaces": {
    "nohoist": [
      "packages/client-web/**"
    ],
    "packages": [
      "projects/api",
      "projects/admin-web",
      "projects/client-web"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "git rev-parse -q --no-revs --verify MERGE_HEAD || lint-staged",
    "setup": "nx run-many --target=setup --all",
    "stack:up": "nx run-many --target=serve --projects=api,admin-web,client-web",
    "release": "./projects/tools/release",
    "release:uat": "./projects/tools/release --target uat",
    "build": "nx run-many --target=build --all --configuration production",
    "lint": "nx run-many --target=lint --all",
    "test": "nx test",
    "db:up": "nx run-many --target=db:up",
    "docker:uat": "env-cmd -f .env.local.uat nx run-many --target=docker --configuration=production --push",
    "docker:production": "env-cmd -f .env.local.production nx run-many --target=docker --configuration=production --push",
    "debug:circular": "madge --circular --extensions=ts projects/api/src/",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "nx": "nx",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.2.1",
    "@nestjs/core": "^10.2.1",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@sentry/node": "^7.17.4",
    "antd": "^5.8.4",
    "assert": "^2.0.0",
    "axios": "^1.0.0",
    "bignumber.js": "^9.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^4.6.0",
    "less": "^4.2.0",
    "moment": "^2.29.4",
    "os": "^0.1.2",
    "pg": "^8.11.3",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.8.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.17",
    "dbmate": "^2.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cypress": "15.8.6",
    "@nrwl/deno": "^0.157.0",
    "@nrwl/devkit": "15.8.0",
    "@nrwl/eslint-plugin-nx": "15.8.6",
    "@nrwl/jest": "15.8.6",
    "@nrwl/js": "15.8.6",
    "@nrwl/linter": "15.8.6",
    "@nrwl/nest": "15.8.6",
    "@nrwl/node": "15.8.6",
    "@nrwl/nx-cloud": "15.1.1",
    "@nrwl/react": "15.8.6",
    "@nrwl/rspack": "15.9.0",
    "@nrwl/web": "15.8.6",
    "@nrwl/webpack": "15.8.6",
    "@nrwl/workspace": "15.8.6",
    "@nx-tools/container-metadata": "4.0.3",
    "@nx-tools/nx-container": "4.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@sentry/types": "7.17.4",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/react": "14.0.0",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "^29.4.1",
    "cypress": "^12.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "15.8.6",
    "prettier": "^2.6.2",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5",
    "url-loader": "^4.1.1"
  },
  "lint-staged": {
    "{apps,projects,packages}/**/*.{ts,tsx}": [
      "prettier --write"
    ]
  }
}
